{
  "name": "@_davideast/stitch-mcp",
  "version": "0.1.0",
  "description": "Stitch MCP OAuth setup assistant - automates Google Cloud authentication for Stitch API",
  "type": "module",
  "bin": {
    "stitch-mcp": "./bin/stitch-mcp.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "bin",
    "README.md"
  ],
  "scripts": {
    "build": "bun build src/index.ts --outdir=dist --target=bun --format=esm",
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build"
  },
  "keywords": [
    "stitch",
    "mcp",
    "oauth",
    "gcloud",
    "authentication",
    "cli"
  ],
  "author": "David East",
  "license": "MIT",
  "dependencies": {
    "@inquirer/prompts": "^7.2.0",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "adm-zip": "^0.5.16",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.5.7",
    "@types/bun": "latest",
    "@types/express": "^5.0.6",
    "@types/node": "^25.0.9",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}